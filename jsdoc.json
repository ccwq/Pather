{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "src/"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "node_modules/jsdoc-export-default-interop/dist/index"
  ],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/",
    "verbose": true,
    "template": "node_modules/docdash",
    "recurse": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}